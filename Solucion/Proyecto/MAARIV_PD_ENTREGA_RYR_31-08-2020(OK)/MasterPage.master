    <script type="text/javascript">
        function add_Datasets(id_table, label_, type_, array_number_datas) {
            //chart recorrer table
            _datas = [];
            //number_datas 
            $('#' + id_table + ' tbody tr').each(function (index) {
                for (var i = 0; i < array_number_datas.length; i++) {
                    _datas.push($(this).children("td")[array_number_datas[i]].innerText);
                }
            });
            return _datas;
        };
        function add_Labels(id_table, array_number_labels) {
            //chart recorrer label
            _labels = [];
            //number_labels 
